<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.controlador.TipoTransaccionController">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="600.0">
         <children>
            <AnchorPane layoutX="4.0" layoutY="4.0" prefHeight="393.0" prefWidth="592.0">
               <children>
                  <Button fx:id="btnDepositar" layoutX="25.0" layoutY="241.0" mnemonicParsing="false" onAction="#OnDepositar" prefHeight="25.0" prefWidth="151.0" text="Depositar">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnRetirar" layoutX="221.0" layoutY="241.0" mnemonicParsing="false" onAction="#OnRetirar" prefHeight="25.0" prefWidth="151.0" text="Retirar">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnConsultarSaldo" layoutX="415.0" layoutY="241.0" mnemonicParsing="false" onAction="#OnConsultarSaldo" prefHeight="25.0" prefWidth="151.0" text="Consultar Saldo">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <Label layoutX="25.0" layoutY="24.0" prefHeight="130.0" prefWidth="553.0" text="¿Que Tipo de transacción desea realizar?" textAlignment="CENTER" wrapText="true">
                     <font>
                        <Font name="Segoe Script" size="30.0" />
                     </font>
                  </Label>
                  <Label layoutX="25.0" layoutY="320.0" prefHeight="73.0" prefWidth="92.0" text="Uni" textFill="#2000d3">
                     <font>
                        <Font name="Segoe Script Bold" size="30.0" />
                     </font>
                  </Label>
                  <Label layoutX="91.0" layoutY="320.0" prefHeight="73.0" prefWidth="143.0" text="Banco">
                     <font>
                        <Font name="Segoe Script Bold" size="30.0" />
                     </font>
                  </Label>
                  <Button fx:id="btnVolver" layoutX="-1.0" layoutY="2.0" mnemonicParsing="false" onAction="#OnVolverAtras" text="Volver" />
               </children>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
            </AnchorPane>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
