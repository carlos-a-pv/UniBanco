<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.controlador.VistaTransaccionesController">
   <children>
      <Pane layoutY="8.0" prefHeight="391.0" prefWidth="600.0">
         <children>
            <TableView fx:id="tbTransacciones" layoutX="15.0" layoutY="30.0" prefHeight="331.0" prefWidth="571.0">
              <columns>
                <TableColumn fx:id="colTipoTransaccion" prefWidth="111.0" text="Tipo Transaccion" />
                <TableColumn fx:id="colValor" prefWidth="64.0" text="Valor" />
                  <TableColumn fx:id="colEstado" prefWidth="120.0" text="Estado" />
                  <TableColumn fx:id="colFecha" prefWidth="75.0" text="Fecha" />
              </columns>
            </TableView>
            <Button fx:id="btnVolverClick" layoutY="-2.0" mnemonicParsing="false" onAction="#OnVolverClick" text="Volver" />
         </children>
      </Pane>
   </children>
</AnchorPane>
